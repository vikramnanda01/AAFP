        <fullName>End_Time__c</fullName>
        <deprecated>false</deprecated>
        <description>** System Calculated ** The scheduled end time.</description>
        <externalId>false</externalId>
        <formula>IF(
  OR(
    VALUE( MID( TEXT( End_Date_Time__c + (VALUE(SUBSTITUTE(SUBSTITUTE(LEFT(TEXT(Event__r.Time_Zone__c), 10),&quot;(GMT&quot;,&quot;&quot;),&quot;:&quot;,&quot;.&quot;))/24) ), 12, 2 ) ) = 0,
    VALUE( MID( TEXT( End_Date_Time__c +  (VALUE(SUBSTITUTE(SUBSTITUTE(LEFT(TEXT(Event__r.Time_Zone__c), 10),&quot;(GMT&quot;,&quot;&quot;),&quot;:&quot;,&quot;.&quot;))/24) ), 12, 2 ) ) = 12
  ),
  &quot;12&quot;,
  TEXT( VALUE( MID( TEXT( End_Date_Time__c +  (VALUE(SUBSTITUTE(SUBSTITUTE(LEFT(TEXT(Event__r.Time_Zone__c), 10),&quot;(GMT&quot;,&quot;&quot;),&quot;:&quot;,&quot;.&quot;))/24)), 12, 2 ) )
   -
   IF(
     VALUE( MID( TEXT( End_Date_Time__c +  (VALUE(SUBSTITUTE(SUBSTITUTE(LEFT(TEXT(Event__r.Time_Zone__c), 10),&quot;(GMT&quot;,&quot;&quot;),&quot;:&quot;,&quot;.&quot;))/24) ), 12, 2 ) ) &lt; 12,
     0,
     12
   )
  )
)
&amp; &quot;:&quot; &amp;
MID( TEXT( End_Date_Time__c +  (VALUE(SUBSTITUTE(SUBSTITUTE(LEFT(TEXT(Event__r.Time_Zone__c), 10),&quot;(GMT&quot;,&quot;&quot;),&quot;:&quot;,&quot;.&quot;))/24) ), 15, 2 )
&amp; &quot; &quot; &amp;
IF(
  VALUE( MID( TEXT( End_Date_Time__c +  (VALUE(SUBSTITUTE(SUBSTITUTE(LEFT(TEXT(Event__r.Time_Zone__c), 10),&quot;(GMT&quot;,&quot;&quot;),&quot;:&quot;,&quot;.&quot;))/24) ), 12, 2 ) ) &lt; 12,
  &quot;AM&quot;,
  &quot;PM&quot;
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>** System Calculated ** The scheduled end time.</inlineHelpText>
