  TEXT( VALUE( MID( TEXT( Start_Date_Time__c +  (VALUE(SUBSTITUTE(SUBSTITUTE(LEFT(TEXT(Event__r.Time_Zone__c), 10),&quot;(GMT&quot;,&quot;&quot;),&quot;:&quot;,&quot;.&quot;))/24)), 12, 2 ) )
   -
