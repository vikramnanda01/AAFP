    <fields>
        <fullName>Preferred_Email__c</fullName>
        <deprecated>false</deprecated>
        <description>**System Calculated** The Preferred Email for the Contact is calculated by mapping the Preferred Email Type to it&apos;s corresponding field&apos;s value.</description>
        <externalId>false</externalId>
        <formula>IF(ISPICKVAL(Preferred_Email_Type__c,&quot;Personal&quot;) &amp;&amp; NOT(Personal_Email_Opt_Out__c), Personal_Email__c,
IF(ISPICKVAL(Preferred_Email_Type__c,&quot;Work&quot;) &amp;&amp; NOT(Work_Email_Opt_Out__c), Work_Email__c,
IF(ISPICKVAL(Preferred_Email_Type__c,&quot;Other&quot;) &amp;&amp; NOT(Other_Email_Opt_Out__c),Other_Email__c,
IF(ISPICKVAL(Preferred_Email_Type__c,&quot;Assistant&quot;) &amp;&amp; NOT(Assistant_Email_Opt_Out__c),Assistant_Email__c, &apos;&apos;))))</formula>
        <inlineHelpText>**System Calculated** The Preferred Email for the Contact is calculated by mapping the Preferred Email Type to it&apos;s corresponding field&apos;s value.</inlineHelpText>
        <label>Preferred Email</label>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
