        <fullName>Renew</fullName>
        <availability>online</availability>
        <description>Creates a Sales Order, Sales Order Lines automatically and redirects page to Sales Order.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Renew</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/30.0/apex.js&quot; )}

var connection = sforce.connection;
var myquery = &quot;SELECT Id, {!$Setup.App_Config__c.Namespace_Prefix__c}Is_Active__c FROM {!$Setup.App_Config__c.Namespace_Prefix__c}Subscription_Plan__c WHERE Id = &apos;{!Subscription__c.Subscription_PlanId__c}&apos;&quot;;
result = sforce.connection.query(myquery);
records = result.getArray(&quot;records&quot;);
if (records[0] &amp;&amp; records[0].{!$Setup.App_Config__c.Namespace_Prefix__c}Is_Active__c === &apos;false&apos;) {
    alert(&apos;Cannot renew with inactive Subscription Plan&apos;);
}
else {
var newOrder= new sforce.SObject(&quot;{!$Setup.App_Config__c.Namespace_Prefix__c}Sales_Order__c&quot;);
newOrder.{!$Setup.App_Config__c.Namespace_Prefix__c}Contact__c = &quot;{!Subscription__c.ContactId__c}&quot;;
newOrder.{!$Setup.App_Config__c.Namespace_Prefix__c}Account__c = &quot;{!Subscription__c.AccountId__c}&quot;;
newOrder.{!$Setup.App_Config__c.Namespace_Prefix__c}Business_Group__c = &quot;{!Subscription__c.Business_GroupId__c}&quot;;
newOrder.{!$Setup.App_Config__c.Namespace_Prefix__c}Entity__c = &quot;{!Subscription__c.Entity__c}&quot;;
result = sforce.connection.create([newOrder]);

var newLine= new sforce.SObject(&quot;{!$Setup.App_Config__c.Namespace_Prefix__c}Sales_Order_Line__c&quot;);
newLine.{!$Setup.App_Config__c.Namespace_Prefix__c}Sales_Order__c = result[0].id;
newLine.{!$Setup.App_Config__c.Namespace_Prefix__c}Is_Renewal__c = &quot;true&quot;;
newLine.{!$Setup.App_Config__c.Namespace_Prefix__c}Item__c = &quot;{!Subscription__c.ItemId__c}&quot;;
newLine.{!$Setup.App_Config__c.Namespace_Prefix__c}Subscription__c = &quot;{!Subscription__c.Id}&quot;;
newLine.{!$Setup.App_Config__c.Namespace_Prefix__c}Business_Group__c = &quot;{!Subscription__c.Business_GroupId__c}&quot;;
newLine.{!$Setup.App_Config__c.Namespace_Prefix__c}Item_Class__c = &quot;{!Subscription__c.Item_ClassId__c}&quot;;
newLine.{!$Setup.App_Config__c.Namespace_Prefix__c}Entity__c = &quot;{!Subscription__c.Entity__c}&quot;;
newLine.{!$Setup.App_Config__c.Namespace_Prefix__c}Enable_Auto_Renew__c = &quot;{!Subscription__c.Enable_Auto_Renew__c}&quot;;
result2 = sforce.connection.create([newLine]);

window.location = &quot;{!$Site.Prefix}/&quot; + result[0].id;
        }</url>
