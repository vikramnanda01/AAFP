<apex:page showHeader="false"
  standardStylesheets="false"
  docType="html-5.0"
  applyHtmlTag="false"
  applyBodyTag="false">

  <apex:composition template="CPBase__template">
    <apex:define name="main">
      <div class="container">
        <div class="row">
          <div class="col-md-6 col-md-offset-3 p-t-40 p-b-40">
            <div class="well">
              <div class="text-center">
                <h1><apex:insert name="title"/></h1>
                <h2><apex:insert name="error"/></h2>
                <p>
                  <apex:insert name="description" />
                </p>
              </div>
            </div>
          </div>
        </div>
      </div>
    </apex:define>
  </apex:composition>
</apex:page>